{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/swagger.json",
      "dest": "/swagger.json"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/api-docs/$1"
    }
  ]
}
